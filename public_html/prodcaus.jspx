                                            action="#{cauastionsManipulation.AddDisabilityLosses}"/>
                          <af:commandButton text="Edit" id="cb10"
                                            icon="/images/update_ena.png"
                                            action="#{cauastionsManipulation.EditDisabilityLosses}"/>
                          <af:commandButton text="Delete" id="cb11"
                                            icon="/images/delete_ena.png"
                                            action="#{cauastionsManipulation.DeleteDisabilityLosses}"/>
