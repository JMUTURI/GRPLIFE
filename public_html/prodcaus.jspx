                            <af:outputText value="#{row.PCT_CODE}" id="ot7"/>
                          </af:column>
                          <af:column sortProperty="PCT_CVT_CODE" sortable="true"
                                     headerText="#{bindings.Setup1.hints.PCT_CVT_CODE.label}"
