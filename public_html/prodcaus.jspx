                                <af:outputLabel value="Division Factor" id="ol12"/>
                                <af:selectOneChoice label="Label 3" id="soc3"
                                                    simple="true"
                                                    shortDesc="Rate Division Factor"
                                                    autoSubmit="true"
                                                    binding="#{cauastionsManipulation.ldlDivisionFactor}">
                                  <af:selectItem label="Per Mille"
                                                 value="1000" id="si7"/>
